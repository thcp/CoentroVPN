{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "coentrovpn_server_helper_sessions",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Active Tunnels",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "displayName": "${__series.name}",
          "unit": "short",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "id": 2,
      "options": {
        "displayMode": "basic",
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false },
        "showUnfilled": true
      },
      "targets": [
        {
          "expr": "increase(coentrovpn_server_helper_sessions_created_total[5m])",
          "legendFormat": "created",
          "refId": "A"
        },
        {
          "expr": "increase(coentrovpn_server_helper_sessions_closed_total[5m])",
          "legendFormat": "closed",
          "refId": "B"
        }
      ],
      "title": "Session Churn (5m)",
      "type": "bargauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15, "stacking": { "mode": "normal" } },
          "unit": "Bps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 3,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "sum by (interface, direction) (rate(coentrovpn_server_helper_bytes_total[1m]))",
          "legendFormat": "{{interface}} {{direction}}",
          "refId": "A"
        }
      ],
      "title": "Helper Throughput (1m rate)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2 },
          "unit": "pps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 4,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "expr": "sum by (interface, direction) (rate(coentrovpn_server_helper_packets_total[1m]))",
          "legendFormat": "{{interface}} {{direction}}",
          "refId": "A"
        },
        {
          "expr": "rate(coentrovpn_server_helper_errors_total{kind=\"tun_queue_full\"}[1m])",
          "legendFormat": "queue_full",
          "refId": "B",
          "yaxis": 2
        }
      ],
      "title": "Packet Rate vs Queue Drops",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": { "displayName": "{{kind}}", "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "id": 5,
      "options": {
        "showHeader": true,
        "footer": { "enablePagination": true }
      },
      "targets": [
        {
          "expr": "sum by (kind) (rate(coentrovpn_server_helper_errors_total[5m]))",
          "legendFormat": "{{kind}}",
          "refId": "A"
        }
      ],
      "title": "Error Rate (5m)",
      "type": "table"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["coentrovpn", "dataplane"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "label": "Prometheus",
        "type": "datasource",
        "query": "prometheus",
        "refresh": 1
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": { "refresh_intervals": ["10s", "30s", "1m", "5m", "10m"] },
  "timezone": "",
  "title": "CoentroVPN Server Helper",
  "uid": "coentrovpn-server-helper",
  "version": 1,
  "weekStart": ""
}
